{
  "name": "tido",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "preinstall": "npx only-allow pnpm",
    "prisma:reset": "pnpm -C apps/api prisma:reset",
    "prisma:migrate": "pnpm -C apps/api prisma:migrate",
    "prisma:generate": "pnpm -C apps/api prisma:generate"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.1",
    "@types/lodash": "^4.14.191",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.54.1",
    "eslint": "^8.47.0",
    "eslint-config-custom": "^0.0.0",
    "prettier": "^2.8.4",
    "tsconfig-paths": "^4.1.2",
    "turbo": "^1.8.3",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@mantine/core": "^6.0.14",
    "@mantine/dates": "^6.0.14",
    "@mantine/dropzone": "^6.0.14",
    "@mantine/form": "^6.0.14",
    "@mantine/hooks": "^6.0.14",
    "@mantine/modals": "^6.0.14",
    "@mantine/notifications": "^6.0.14",
    "@mantine/prism": "^6.0.14",
    "@mantine/spotlight": "^6.0.14",
    "dayjs": "^1.11.8",
    "lodash": "^4.17.21",
    "zod": "^3.20.2"
  }
}
