{
  "name": "tido",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "preinstall": "npx only-allow pnpm",
    "db:reset": "pnpm -C apps/api db:reset",
    "db:migrate": "pnpm -C apps/api db:migrate",
    "db:generate": "pnpm -C apps/api db:generate",
    "db:push": "pnpm -C apps/api db:push"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "@types/lodash": "^4.14.197",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.48.0",
    "eslint-config-custom": "^0.0.0",
    "prettier": "^2.8.8",
    "tsconfig-paths": "^4.2.0",
    "turbo": "^1.10.13",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@mantine/core": "^6.0.19",
    "@mantine/dates": "^6.0.19",
    "@mantine/dropzone": "^6.0.19",
    "@mantine/form": "^6.0.19",
    "@mantine/hooks": "^6.0.19",
    "@mantine/modals": "^6.0.19",
    "@mantine/notifications": "^6.0.19",
    "@mantine/prism": "^6.0.19",
    "@mantine/spotlight": "^6.0.19",
    "dayjs": "^1.11.9",
    "lodash": "^4.17.21",
    "zod": "^3.22.2"
  }
}
