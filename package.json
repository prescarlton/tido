{
  "name": "tido",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "preinstall": "npx only-allow pnpm",
    "prisma:reset": "pnpm -C apps/api prisma:reset",
    "prisma:migrate": "pnpm -C apps/api prisma:migrate",
    "prisma:generate": "pnpm -C apps/api prisma:generate"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.1",
    "@types/lodash": "^4.14.191",
    "eslint-config-custom": "^0.0.0",
    "prettier": "^2.8.4",
    "tsconfig-paths": "^4.1.2",
    "turbo": "^1.8.3"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "typescript": "^4.9.5",
    "zod": "^3.20.2"
  }
}
